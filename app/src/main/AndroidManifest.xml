<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.hackaton.hevre.clientapplication">

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <!-- Needed only if your app targets Android 5.0 (API level 21) or higher. -->
    <uses-feature android:name="android.hardware.location.gps" />
    <!--<uses-sdk tools:overrideLibrary="me.originqiu.library"/>-->

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".Controller.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Controller.HomeActivity"
            android:label="@string/title_activity_home" >
        </activity>
        <activity
            android:name=".Controller.RegisterActivity"
            android:label="@string/title_activity_register" >
        </activity>
        <activity
            android:name=".Controller.TaskActivity"
            android:label="@string/title_activity_task" >
        </activity>
    </application>

</manifest>